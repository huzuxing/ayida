package com.ayida.cms.entity.doctor;

public class DoctorExt extends BaseDoctor
{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private Integer job;
	
	private String department;
	
	private Integer professionalTitles;
	
	private String skilledField;
	
	private String certification;
	
	private String comment;
	
	private String img;
	
	public DoctorExt()
	{
		super();
	}

	public DoctorExt(Integer id, String name, Integer job, String department,
			Integer professionalTitles, String skilledField,
			String certification, String comment, String img)
	{
		super(id, name);
		this.job = job;
		this.department = department;
		this.professionalTitles = professionalTitles;
		this.skilledField = skilledField;
		this.certification = certification;
		this.comment = comment;
		this.img = img;
	}

	/**
	 * @return the ranks
	 */
	public Integer getJob()
	{
		return job;
	}

	/**
	 * @param ranks the ranks to set
	 */
	public void setJob(Integer job)
	{
		this.job = job;
	}

	/**
	 * @return the department
	 */
	public String getDepartment()
	{
		return department;
	}

	/**
	 * @param department the department to set
	 */
	public void setDepartment(String department)
	{
		this.department = department;
	}

	/**
	 * @return the professionalTitles
	 */
	public Integer getProfessionalTitles()
	{
		return professionalTitles;
	}

	/**
	 * @param professionalTitles the professionalTitles to set
	 */
	public void setProfessionalTitles(Integer professionalTitles)
	{
		this.professionalTitles = professionalTitles;
	}

	/**
	 * @return the skilledField
	 */
	public String getSkilledField()
	{
		return skilledField;
	}

	/**
	 * @param skilledField the skilledField to set
	 */
	public void setSkilledField(String skilledField)
	{
		this.skilledField = skilledField;
	}

	/**
	 * @return the certification
	 */
	public String getCertification()
	{
		return certification;
	}

	/**
	 * @param certification the certification to set
	 */
	public void setCertification(String certification)
	{
		this.certification = certification;
	}

	/**
	 * @return the comment
	 */
	public String getComment()
	{
		return comment;
	}

	/**
	 * @param comment the comment to set
	 */
	public void setComment(String comment)
	{
		this.comment = comment;
	}

	/**
	 * @return the img
	 */
	public String getImg()
	{
		return img;
	}

	/**
	 * @param img the img to set
	 */
	public void setImg(String img)
	{
		this.img = img;
	}
	

}
